ent) => {\n    setSearch(event.target.value);\n  };\n\n  return (\n    <>\n      {/* Display login and signup modal when necessary */}\n      {modal.loginModal && (\n        <LoginModal modal={modal} setModal={setModal} setUser={setUser} />\n      )}\n      {modal.signupModal && (\n        <SignupModal modal={modal} setModal={setModal} setUser={setUser} />\n      )}\n      <div className=\"header\">\n        <div className=\"container\">\n          <div className=\"header-content\">\n            <div className=\"header-left\">\n              <Link to=\"/\" className=\"header-logo\">\n                <img src={MarvelLogo} alt=\"marvel-logo\" />\n              </Link>\n              <div className=\"header-search-bar\">\n                <form>\n                  <i>\n                    <FontAwesomeIcon icon=\"search\" size=\"2x\" />\n                  </i>\n                  <input\n                    type=\"text\"\n                    value={search}\n                    onChange={handleSearchChange}\n                    placeholder=\"Rechercher\"\n                  />\n                </form>\n              </div>\n            </div>\n            <div className=\"header-right\">\n              <div className=\"header-menu\">\n                <nav>\n                  <Link to=\"/characters\">CHARACTERS</Link>\n                  <Link to=\"/comics\">COMICS</Link>\n                  <Link to=\"/favorites\">FAVORITES</Link>\n                </nav>\n              </div>\n              {/* If user connected display of a logout button */}\n              {userInfo.token ? (\n                <button\n                  className=\"Button-logout\"\n                  onClick={() => {\n                    setUser(null);\n                    const newModal = { ...modal };\n                    newModal.loginModal = !modal.loginModal;\n                    setModal(newModal);\n                    history.push(\"/\");\n                  }}\n                >\n                  Se d√©connecter\n                </button>\n              ) : (\n                // If user not connected diplay of the login button\n                <button\n                  onClick={() => {\n                    const newModal = { ...modal };\n                    newModal.loginModal = !modal.loginModal;\n                    setModal(newModal);\n                  }}\n                  className=\"Button-login\"\n                >\n                  Se connecter\n                </button>\n              )}\n            </div>\n          </div>\n        </div>\n      </div>\n    </>\n  );\n};\n\nexport default Header;\n","/Users/Thomas/LeReacteur/React/Marvel/marvel-frontend/src/components/characterCard/characterCard.js",["72"],"import \"./character-card.scss\";\nimport { Link } from \"react-router-dom\";\nimport { FontAwesomeIcon } from \"@fortawesome/react-fontawesome\";\nimport { useState, useEffect } from \"react\";\n\nconst CharacterCard = ({ characterData }) => {\n  const localStorage = window.localStorage;\n  const [isFavorite, setIsFavorite] = useState(false);\n\n  const handleFavorite = () => {\n    const favorites = JSON.parse(localStorage.getItem(\"favCharacters\")) || [];\n    const index = favorites.findIndex((elem) => {\n      return elem._id === characterData._id;\n    });\n    index === -1 ? favorites.push(characterData) : favorites.splice(index, 1);\n    localStorage.setItem(\"favCharacters\", JSON.stringify(favorites));\n    setIsFavorite(!isFavorite);\n  };\n\n  useEffect(() => {\n    const favorites = JSON.parse(localStorage.getItem(\"favCharacters\")) || [];\n    favorites.findIndex((elem) => {\n      return elem._id === characterData._id;\n    }) !== -1 && setIsFavorite(true);\n  }, []);\n\n  return (\n    <div className=\"character-card\">\n      <Link\n        to={{ pathname: `/character/${characterData._id}` }}\n        className=\"character-card-content\"\n      >\n        <img\n          src={`${characterData.thumbnail.path}.${characterData.thumbnail.extension}`}\n          alt=\"marvel-character\"\n        />\n        {isFavorite && (\n          <Link\n            to=\"#\"\n            className=\"character-card-favorite\"\n            onClick={handleFavorite}\n            style={\n              isFavorite ? { color: \"rgb(218, 218, 75)\" } : { color: \"white\" }\n            }\n          >\n            <FontAwesomeIcon icon=\"star\" size=\"2x\" />\n          </Link>\n        )}\n\n        <div className=\"character-card-information\">\n          <Link\n            to=\"#\"\n            className=\"character-card-favorite-hover\"\n            onClick={handleFavorite}\n          >\n            <FontAwesomeIcon icon=\"star\" size=\"2x\" />\n          </Link>\n          <div className=\"character-card-details\">\n            <h2>{characterData.name}</h2>\n            <p>{characterData.description}</p>\n          </div>\n        </div>\n      </Link>\n    </div>\n  );\n};\n\nexport default CharacterCard;\n","/Users/Thomas/LeReacteur/React/Marvel/marvel-frontend/src/components/isLoading/isLoading.js",[],"/Users/Thomas/LeReacteur/React/Marvel/marvel-frontend/src/components/comicCard/comicCard.js",["73"],"import \"./comic-card.scss\";\nimport { Link } from \"react-router-dom\";\nimport { FontAwesomeIcon } from \"@fortawesome/react-fontawesome\";\nimport { useState, useEffect } from \"react\";\n\nconst ComicCard = ({ comicData }) => {\n  const localStorage = window.localStorage;\n  const [isFavorite, setIsFavorite] = useState(false);\n\n  const handleFavorite = () => {\n    const favorites = JSON.parse(localStorage.getItem(\"favComics\")) || [];\n    const index = favorites.findIndex((elem) => {\n      return elem._id === comicData._id;\n    });\n    index === -1 ? favorites.push(comicData) : favorites.splice(index, 1);\n    localStorage.setItem(\"favComics\", JSON.stringify(favorites));\n    setIsFavorite(!isFavorite);\n  };\n\n  useEffect(() => {\n    const favorites = JSON.parse(localStorage.getItem(\"favComics\")) || [];\n    favorites.findIndex((elem) => {\n      return elem._id === comicData._id;\n    }) !== -1 && setIsFavorite(true);\n  }, []);\n\n  return (\n    <div className=\"comic-card\">\n      <Link\n        to={{ pathname: \"/comics\", data: { comicData } }}\n        className=\"comic-card-content\"\n      >\n        <img\n          src={`${comicData.thumbnail.path}.${comicData.thumbnail.extension}`}\n          alt=\"marvel-comic\"\n        />\n        {isFavorite && (\n          <Link\n            to=\"#\"\n            className=\"comic-card-favorite\"\n            onClick={handleFavorite}\n            style={\n              isFavorite ? { color: \"rgb(218, 218, 75)\" } : { color: \"white\" }\n            }\n          >\n            <FontAwesomeIcon icon=\"star\" size=\"2x\" />\n          </Link>\n        )}\n        <div className=\"comic-card-information\">\n          <Link\n            to=\"#\"\n            className=\"comic-card-favorite-hover\"\n            onClick={handleFavorite}\n            style={\n              isFavorite ? { visibility: \"hidden\" } : { visibility: \"visible\" }\n            }\n          >\n            <FontAwesomeIcon icon=\"star\" size=\"2x\" />\n          </Link>\n          <div className=\"comic-card-details\">\n            <h2>{comicData.title}</h2>\n            <p>{comicData.description}</p>\n          </div>\n        </div>\n      </Link>\n    </div>\n  );\n};\n\nexport default ComicCard;\n","/Users/Thomas/LeReacteur/React/Marvel/marvel-frontend/src/components/pagging/pagging.js",[],"/Users/Thomas/LeReacteur/React/Marvel/marvel-frontend/src/components/paggingLimit/paggingLimit.js",[],"/Users/Thomas/LeReacteur/React/Marvel/marvel-frontend/src/containers/favorites/favorites.js",[],"/Users/Thomas/LeReacteur/React/Marvel/marvel-frontend/src/components/loginModal/loginModal.js",[],"/Users/Thomas/LeReacteur/React/Marvel/marvel-frontend/src/components/signupModal/signupModal.js",[],"/Users/Thomas/LeReacteur/React/Marvel/marvel-f